<!-- 
The master of this file is /icons/drawables-tinyglance.xml
It is automatically copied by generate.js into each device resource folder, since it needs to reside in the same folder as the image files it refers to.
-->
<drawables>
    <bitmap id="logo_flash" filename="logo_flash.png" packingFormat="png"/>
    <bitmap scope="foreground" id="logo_evcc" filename="logo_evcc.png" packingFormat="png"/>

    <!-- glance font size, only icons needed in the glances -->
    <bitmap scope="glance" id="battery_empty_glance" filename="icon_glance_battery_empty.png" packingFormat="png">
        <palette disableTransparency="true">
            <color>FFFFFF</color>
            <color>AAAAAA</color>
            <color>555555</color>
            <color>000000</color>    
        </palette>
    </bitmap>
